<div id="<%= dom_id event %>">
  <p>
    <strong>Event type:</strong>
    <%= event.event_type %>
  </p>

  <p>
    <strong>Timestamp:</strong>
    <%= event.timestamp %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= event.description %>
  </p>

  <p>
    <strong>Severity:</strong>
    <%= event.severity %>
  </p>

</div>
