<div id="<%= dom_id prediction %>">
  <p>
    <strong>Host:</strong>
    <%= prediction.host_id %>
  </p>

  <p>
    <strong>Threat actor:</strong>
    <%= prediction.threat_actor_id %>
  </p>

  <p>
    <strong>Context:</strong>
    <%= prediction.context %>
  </p>

  <p>
    <strong>Confidence:</strong>
    <%= prediction.confidence %>
  </p>

</div>
